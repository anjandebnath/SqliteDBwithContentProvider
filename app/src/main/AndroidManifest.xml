<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anjan.user06.sqlitedbwithcontentprovider">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".contentprovider.StudentProvider"
            android:authorities="com.anjan.user06.sqlitedbwithcontentprovider.contentprovider"
            android:exported="true"
            android:grantUriPermissions="true"
            android:label="CustomContentProvider"
            android:readPermission="com.anjan.user06.sqlitedbwithcontentprovider.READ"
            android:writePermission="com.anjan.user06.sqlitedbwithcontentprovider.WRITE" />

    </application>

</manifest>